int main() {
    int n
    n := input()
    array data[n]
    if ((n < 1)) {
        print("n should be greater than 1")
        return -1    
    }
    data[0] := 2
    int i, x
    i := 1
    x := 3
    while ((i < n)) {
        int j, flag
        j := 0
        flag := 0
        while ((j < i)) {
            if (((x % data[j]) == 0)) {
                flag := 1    
            }
            j := (j + 1)
        }
        if ((flag == 0)) {
            data[i] := x
            i := (i + 1)
        }
        x := (x + 1)
    }
    
    print(data[(n - 1)], "\n")

    return 0
}
